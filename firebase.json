{
  "hosting": {
    "public": "dist/apps/ngx-bootstrap-docs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/ngx-bootstrap/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/ngx-bootstrap/",
        "type": 302
      }
    ]
  }
}
