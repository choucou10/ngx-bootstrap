{
  "npmScope": "ngx-bootstrap-base",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "ngx-bootstrap-docs": { "tags": [], "implicitDependencies": [
      "accordion", "alert", "buttons", "carousel", "chronos", "collapse", "datepicker",
      "dropdown", "locale", "modal", "pagination", "popover", "progressbar", "rating",
      "sortable", "tabs", "timepicker", "tooltip", "typeahead"
    ] },
    "ngx-bootstrap-docs-e2e": {
      "tags": [],
      "implicitDependencies": ["ngx-bootstrap-docs"]
    },
    "accordion": { "tags": ["lib"], "implicitDependencies": ["utils", "collapse"] },
    "alert": { "tags": ["lib"], "implicitDependencies": ["utils"] },
    "buttons": { "tags": ["lib"] },
    "carousel": { "tags": ["lib"], "implicitDependencies": ["utils"] },
    "chronos": { "tags": ["lib"] },
    "collapse": { "tags": ["lib"] },
    "component-loader": { "tags": ["lib"], "implicitDependencies": ["utils", "positioning"] },
    "datepicker": { "tags": ["lib"], "implicitDependencies": ["mini-ngrx", "tooltip"] },
    "dropdown": { "tags": ["lib"], "implicitDependencies": ["positioning", "component-loader"] },
    "locale": { "tags": ["lib"], "implicitDependencies": ["chronos"] },
    "mini-ngrx": { "tags": ["lib"] },
    "modal": { "tags": ["lib"], "implicitDependencies": ["positioning", "component-loader"] },
    "pagination": { "tags": ["lib"] },
    "popover": { "tags": ["lib"], "implicitDependencies": ["positioning", "component-loader"] },
    "positioning": { "tags": ["lib"] },
    "progressbar": { "tags": ["lib"], "implicitDependencies": ["utils"] },
    "rating": { "tags": ["lib"] },
    "sortable": { "tags": ["lib"] },
    "tabs": { "tags": ["lib"] },
    "timepicker": { "tags": ["lib"], "implicitDependencies": ["mini-ngrx"] },
    "tooltip": { "tags": ["lib"], "implicitDependencies": ["positioning", "component-loader"] },
    "typeahead": { "tags": ["lib"], "implicitDependencies": ["positioning", "component-loader"]  },
    "utils": { "tags": ["lib"] }
  }
}
